<table class='table'>
  <thead>
    <tr>
      <th>Group</th>
      <th>Message</th>
      <th>When</th>
    </tr>
  </thead>
  <tbody>
    <% stored_exceptions.each do |se| %>
      <tr>
        <td><%= link_to(group_id(se.group_id), se.group)%></td>
        <td class="exception-canary-truncated"><%= link_to(se.title, se) %></td>
        <td style="white-space: nowrap;"><%= format_time_ago(se.created_at) %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<% if defined? stored_exceptions.current_page %>
  <%= paginate stored_exceptions %>
<% end %>